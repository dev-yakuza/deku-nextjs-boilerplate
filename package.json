{
  "name": "deku-nextjs-boilerplate",
  "description": "Boilerplate for NextJS, MUI, Emotion, Storybook, React Query, Recoil",
  "version": "1.0.0",
  "keywords": [
    "boilerplate",
    "nextjs",
    "mui",
    "emotion",
    "storybook",
    "react-query",
    "recoil"
  ],
  "author": "dev-yakuza <dev.yakuza@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-yakuza/deku-nextjs-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/dev-yakuza/react-native-image-modal/issues"
  },
  "homepage": "https://github.com/dev-yakuza/react-native-image-modal#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.(css|tsx)'",
    "lint:md": "remark . --quiet --frail",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "check:spell": "cspell '**' --gitignore --no-progress ",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.8.0",
    "@mui/x-data-grid": "^5.10.1",
    "axios": "^0.27.2",
    "next": "12.1.6",
    "next-translate": "^1.4.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-query": "^3.39.0",
    "recoil": "^0.7.3-alpha.2"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/core": "^7.17.10",
    "@emotion/jest": "^11.9.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.5.0-beta.8",
    "@storybook/manager-webpack5": "^6.5.0-beta.8",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.5",
    "cspell": "^6.0.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-storybook": "^0.5.11",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "remark-cli": "^10.0.1",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "sb": "^6.4.22",
    "storybook-addon-next-router": "^3.1.1",
    "stylelint": "^14.8.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.6.4"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  }
}
